@font-face{
  font-family:'icomoon';
  src:url("./fonts/icomoon.eot?yisc3l");
  src:url("./fonts/icomoon.eot?yisc3l#iefix") format("embedded-opentype"),
  url("./fonts/icomoon.woff2?yisc3l") format("woff2"),
  url("./fonts/icomoon.ttf?yisc3l") format("truetype"),
  url("./fonts/icomoon.woff?yisc3l") format("woff"),
  url("./fonts/icomoon.svg?yisc3l#icomoon") format("svg");
}
*
  margin 0
  padding 0
a 
  text-decoration none
  color rgb(115,127,142)
.finance-wrapper
  height 102px
  width 388px
  border 1px solid #CAE0F3
  border-top 3px solid #9EC9EE
  overflow hidden
.sleft
  float left
  width 182px
  margin 6px 0 5px 12px
  border-right 1px solid #BDDAF5
.sright
  float left
  margin 6px 0 5px 12px
  // width 182px
.st-title 
  color: #455569;
  width 100%
  font-size 13px
.st-item.st-item-t
  margin-bottom 5px
.st-item .detail
    margin-top: 0;
.st-item .detail .price
  font-size 12px
  display inline-block
  width 85px
.st-item .detail .float,.st-item .detail .percent
    display: inline-block;
    font-family: Arial;
    font-size: 12px;
.st-item .detail .detail-down span 
  color: #2DAA50
.st-item .detail span 
  color  #df4d42


